    <task id="t_filter_search_by_tag">
      <title>Filter Search by Collection or Content tag</title>
      <shortdesc>Filter content searches in Studio by Collection tag.</shortdesc>
      <taskbody>
        <context id="context_ens_jn4_lbb">
          <draft-comment author="ind14750" otherprops="merge">c_content_collection_tags.dita</draft-comment>
        </context>
        <steps id="steps_qyx_3n4_lbb">
          <step>
            <cmd>Select <uicontrol>Tag</uicontrol> in the search dropdown of the
                <uicontrol>Collections</uicontrol> panel.</cmd>
          </step>
          <step>
            <cmd>Entering the tag for your search or click on the <uicontrol>Tag</uicontrol> listed
              under a Collection.</cmd>
            <stepresult>All Collections with the selected Tag will be displayed in the
              list.</stepresult>
          </step>
        </steps>
      </taskbody>
    </task>
    <task id="t_filter_search_by_content_tag">
